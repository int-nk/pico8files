function retry()
	if state=="game" then
		tmax=t+120
		startgame()
	end
	
end


function victory()
		state="win"
		tmax=t+120
		wipe.s="win"
		sfx(6)
end

function draw_parts()
	for pa in all(part) do
		if pa.c==11 then
			circ(pa.x,pa.y,pa.r,10)
		else
			circfill(pa.x,pa.y,pa.r,pa.c)
		end
		
		pa.y-=pa.dy
		pa.r+=pa.dr
		pa.life-=1
		if pa.life<=0 then
					del(part,pa)
		end
		
	end
end
